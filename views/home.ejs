<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="../views/style.css">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
    integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

  <title>Best Playlist</title>
</head>

<body>

  <%-include("./partialNav")%>
  <!-- section 1  -->

  <div class="container">
    <%  if (typeof failed !== 'undefined' && failed) { %>
    <h5> <%= failed %> </h5>
    <%  } %>
    <div class="row" style="margin-top: 200px;">
      <div class="col-4">
        <div class="card ml-auto" style="width: 18rem">
          <img
            src="https://media.istockphoto.com/vectors/letter-o-with-music-logo-icon-design-vector-id1090924366?k=6&m=1090924366&s=170667a&w=0&h=PbvaUAqEg2CzuQsuKU-8lwtF6Pa703iGUm5dO1n-FXc="
            style="height:214px;" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">View List </h5>
            <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
            <a href="playlist" class="btn btn-primary">View all</a>
          </div>
        </div>
      </div>
      <div class="col-4">
        <div class="card mx-auto" style="width: 18rem;">
          <img src="https://www.designyourway.net/blog/wp-content/uploads/2018/12/fox_music_2-700x525.jpg"
            class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">My Playlist</h5>
            <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
            <a href="1/myPlaylist" class="btn btn-primary">Show My Playlist</a>
          </div>
        </div>
      </div>
      <div class="col-4">
        <div class="card mr-auto" style="width: 18rem;">
          <img src="https://i.pinimg.com/originals/fe/c1/18/fec1188fe9a1cec705c0c2489559f0fd.png" class="card-img-top"
            style="height: fit-content;" alt="...">
          <div class="card-body">
            <h5 class="card-title">Add New Playlist</h5>
            <!-- <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p> -->
            <a href="#" class="btn btn-primary">Add New Playlist</a>
          </div>
        </div>
      </div>
    </div>

  </div>

  <!-- end section 1 -->

  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
    integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n"
    crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
    integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
    crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"
    integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6"
    crossorigin="anonymous"></script>
</body>

</html>